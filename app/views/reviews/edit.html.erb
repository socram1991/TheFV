<div class="col-md-6 col-md-offset-3">

  <div class="new-attraction-form">

    <h1>Edit Review</h1>

    <%= simple_form_for([@attraction, @review]) do |f| %>
        <div id="rating-form">
          <label>Rating</label>
        </div>  <%= f.input :comment %>
      <%= f.button :submit %>
    <% end %>

  </div>

</div>
<script>
    $('#rating-form').raty({
        path: '/assets/',
        scoreName: 'review[rating]'
    });
</script>